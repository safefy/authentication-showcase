    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Separator -->
    <style name="Separator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="Separator.DarkGrey" parent="Separator">
        <item name="android:background">@color/dark_grey</item>
    </style>

    <style name="Separator.Grey" parent="Separator">
        <item name="android:background">@color/grey</item>
    </style>

    <style name="Separator.LightGrey" parent="Separator">
        <item name="android:background">@color/light_grey</item>
    </style>

    <style name="MyCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/kf_red</item>
        <item name="colorControlActivated">@color/kf_red</item>
    </style>

</resources>
